                        isRecognizedCustomer: false,
                        deviceType: "web",
                        isOneClick: false,
                        clientPage: "DPX",
                        shouldPopulateCountrySelector: shouldPopulateCountrySelector
                    });
                }
            });

            P.when("LUXController").execute(function(LUXController) {
                LUXController.init({
                    addressLabel : "",
                    addressId    : "",
                    obfuscatedId : "",
