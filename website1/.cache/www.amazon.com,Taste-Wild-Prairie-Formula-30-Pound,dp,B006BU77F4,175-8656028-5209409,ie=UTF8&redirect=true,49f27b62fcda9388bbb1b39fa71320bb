                'colorToAsin': {'initial': {}},
                'heroImage': {'initial': []},
                'heroVideo': {'initial': []},
                'holderRatio': 1.0,
                'holderMaxHeight': 700,
                'useStretchyImageFix': true,
                'isAltImageAutoCycleEnabled': false,
                'altImageAutoCycleInterval': 0,
                'isEnhancedImageBlockSize' : false,
                'preloadingMainBlockForSoftlines' : false
                };
    A.trigger('P.AboveTheFold'); // trigger ATF event.
    return data;
});
</script>




	                
                
            
        
        
        
        
        
        
    
    
    

    






    










