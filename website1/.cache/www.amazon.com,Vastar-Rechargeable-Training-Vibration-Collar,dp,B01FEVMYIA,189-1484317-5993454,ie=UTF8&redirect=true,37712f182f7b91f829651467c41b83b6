        if(typeof performance!= "undefined" && typeof performance.getEntries != "undefined") {
            var dpmaUrl = "https://images-na.ssl-images-amazon.com/images/I/61I9obeY0RL._RC|31bEtOe-akL.css,11ikU6MX1JL.css,31BXJBpf4hL.css,11FgAc1zMiL.css,01Tw+-0+4VL.css,113FuDtwChL.css,21xaajvpfWL.css,11xRy3bSkOL.css,41X5qkJTYhL.css,01FeJyZlLzL.css,01aViOmOIXL.css,11-Y1SbaPUL.css,219d619rUYL.css,01QUs5FVXoL.css,11MXghh1eBL.css,01hvfEvfPgL.css,31G+9aDciKL.css,41A3MwXNi4L.css,31DJFjtqQ7L.css,01GT2VH57dL.css,21Rjwbph1AL.css,21mMd8pV7RL.css,01rgQ3jqo7L.css,11X6RJu0Y4L.css,313hLbpDuZL.css,11CcQ9BObHL.css,219GlkHN2eL.css,11MvtoCY7WL.css_.css";
            var metaAssetName = "USHardlinesDetailPageMetaAsset";
            if(dpmaUrl && metaAssetName) {
                var dpEntry = performance.getEntriesByName(encodeURI(dpmaUrl));
                if(dpEntry && dpEntry[0]) {
                    var dpmaDuration = dpEntry[0].duration;
                    ue.count(metaAssetName +".duration", dpmaDuration);
                    if(dpmaDuration < 50) {
                        ue.tag(metaAssetName + "Cached");
                    } else {
                        ue.tag(metaAssetName + "NotCached");
                    }
                    ue.count(metaAssetName + ".startTime", dpEntry[0].startTime);
                }
            }

            for(i = 0 ; i < performance.getEntries().length ; i++) {
                var name = performance.getEntries()[i].name ;
                var res = name.match(/\/AmazonUI-\S*\.css/g);

                if(res) {
                    var duration = performance.getEntries()[i].duration ;
                    ue.count("aui.duration", duration);
                    ue.count("aui.startTime", performance.getEntries()[i].startTime);
                    if(duration < 50) {
                        csmTag = "auiCached";
                    }
                    else {
                        csmTag = "auiNotCached";
                    }

                    if(window.ue && ue.tag) {
                        ue.tag(csmTag);
                    }
                    break;
                }
            }
        }
