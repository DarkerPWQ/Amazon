    <script type="text/javascript">
    P.when("jQuery", "ready").execute(function($) {
        var url = $("#cerberus-metrics").attr("value");
        $.ajax({ url: url, type: "POST" });

        $(".cerberus-asin").bind("click", function() {
            var url = $(this).attr("data-params");
            $.ajax({ url: url, type: "POST" });
        });

        $("#cerberusVoteYes").bind("click", function() {
            vote($("#cerberusVoteSection").attr("data-yes"));
        });

        $("#cerberusVoteNo").bind("click", function() {
            vote($("#cerberusVoteSection").attr("data-no"));
        });

        var vote = function(url) {
            $.ajax({ url: url, type: "POST" });
            $("#cerberusVoteSection").hide();
            $("#cerberusVoteThankYou").show();
        };
    });

    P.when("jQuery", "p13n-sc-line-truncator").execute(function($, lineTruncator) {
        $(".cerberus-no-wrap").removeClass("cerberus-no-wrap");
        lineTruncator.truncateLines($("#cerberus"));
    });
    </script>

