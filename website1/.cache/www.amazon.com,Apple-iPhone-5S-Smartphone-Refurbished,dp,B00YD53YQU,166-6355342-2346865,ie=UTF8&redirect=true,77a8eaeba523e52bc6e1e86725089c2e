                    var popover = widgetDom.find(".a-popover-trigger");
                    popover.click();
                    P.when("reviewsLightbox-ready-R3VETSP2B9TADW_gallerySection_D1D9F016-5A47-11E6-9C5B-FAE842AC0FC4").execute(function() {
