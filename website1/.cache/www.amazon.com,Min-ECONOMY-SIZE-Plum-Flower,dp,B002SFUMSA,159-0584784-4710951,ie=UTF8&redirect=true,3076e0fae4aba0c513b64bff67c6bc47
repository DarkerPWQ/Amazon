<script type="text/javascript">

window.AmazonPopoverImages = {
  snake: 'https://images-na.ssl-images-amazon.com/images/G/01/javascripts/lib/popover/images/snake._CB192571611_.gif',
  btnClose: 'https://images-na.ssl-images-amazon.com/images/G/01/javascripts/lib/popover/images/btn_close._CB192188154_.gif',
  closeTan: 'https://images-na.ssl-images-amazon.com/images/G/01/nav2/images/close-tan-sm._CB192185930_.gif',
