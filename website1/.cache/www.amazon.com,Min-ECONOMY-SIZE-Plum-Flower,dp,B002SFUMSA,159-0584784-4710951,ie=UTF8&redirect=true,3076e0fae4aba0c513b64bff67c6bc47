<script type="text/javascript">
  var xhr;
  if (window.XMLHttpRequest) {
    xhr = new XMLHttpRequest();
  } else {
    xhr = new ActiveXObject("Microsoft.XMLHTTP");
  }

  xhr.onreadystatechange = function() {
    if(window.ue && ue.tag) {
      if (xhr.readyState == 4) {
        if (xhr.status != 200) {
          ue.tag('sdchDictAjaxFailed');
        } else {
          ue.tag('sdchDictAjaxSuccess');
        }
      } 
    }
  }

  window.addEventListener("load", function(event) {
    setTimeout(function() {
      xhr.open("GET", "/gp/sdch/get.dct", true);
      xhr.send();
      ue.tag('sdchDictAjaxTriggered');
    }, 0);
  });
</script>
