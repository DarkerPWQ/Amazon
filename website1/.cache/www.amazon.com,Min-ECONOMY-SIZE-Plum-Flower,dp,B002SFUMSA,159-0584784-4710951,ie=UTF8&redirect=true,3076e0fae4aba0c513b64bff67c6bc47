    var data = {
                'colorImages': { 'initial': [{"hiRes":"https://images-na.ssl-images-amazon.com/images/I/819pzxqjJZL._SL1500_.jpg","thumb":"https://images-na.ssl-images-amazon.com/images/I/41h9utS1UUL._SS40_.jpg","large":"https://images-na.ssl-images-amazon.com/images/I/41h9utS1UUL.jpg","main":{"https://images-na.ssl-images-amazon.com/images/I/819pzxqjJZL._SY355_.jpg":[186,355],"https://images-na.ssl-images-amazon.com/images/I/819pzxqjJZL._SY450_.jpg":[236,450],"https://images-na.ssl-images-amazon.com/images/I/819pzxqjJZL._SY550_.jpg":[289,550],"https://images-na.ssl-images-amazon.com/images/I/819pzxqjJZL._SY606_.jpg":[318,606],"https://images-na.ssl-images-amazon.com/images/I/819pzxqjJZL._SY679_.jpg":[357,679]},"variant":"MAIN"},{"hiRes":null,"thumb":"https://images-na.ssl-images-amazon.com/images/I/31EpV-41EIL._SS40_.jpg","large":"https://images-na.ssl-images-amazon.com/images/I/31EpV-41EIL.jpg","main":{"https://images-na.ssl-images-amazon.com/images/I/31EpV-41EIL._SY355_.jpg":[266,355],"https://images-na.ssl-images-amazon.com/images/I/31EpV-41EIL._SY450_.jpg":[338,450],"https://images-na.ssl-images-amazon.com/images/I/31EpV-41EIL.jpg":[375,500]},"variant":"PT12"},{"hiRes":"https://images-na.ssl-images-amazon.com/images/I/71ocdIIId2L._SL1315_.jpg","thumb":"https://images-na.ssl-images-amazon.com/images/I/51Gj-DhHT6L._SS40_.jpg","large":"https://images-na.ssl-images-amazon.com/images/I/51Gj-DhHT6L.jpg","main":{"https://images-na.ssl-images-amazon.com/images/I/71ocdIIId2L._SX355_.jpg":[355,220],"https://images-na.ssl-images-amazon.com/images/I/71ocdIIId2L._SX450_.jpg":[450,279],"https://images-na.ssl-images-amazon.com/images/I/71ocdIIId2L._SX425_.jpg":[425,263],"https://images-na.ssl-images-amazon.com/images/I/71ocdIIId2L._SX466_.jpg":[466,288],"https://images-na.ssl-images-amazon.com/images/I/71ocdIIId2L._SX522_.jpg":[522,323]},"variant":"FACT"},{"hiRes":"https://images-na.ssl-images-amazon.com/images/I/51RJhTexgxL._SL1330_.jpg","thumb":"https://images-na.ssl-images-amazon.com/images/I/41cIS1rXt4L._SS40_.jpg","large":"https://images-na.ssl-images-amazon.com/images/I/41cIS1rXt4L.jpg","main":{"https://images-na.ssl-images-amazon.com/images/I/51RJhTexgxL._SX355_.jpg":[355,69],"https://images-na.ssl-images-amazon.com/images/I/51RJhTexgxL._SX450_.jpg":[450,88],"https://images-na.ssl-images-amazon.com/images/I/51RJhTexgxL._SX425_.jpg":[425,83],"https://images-na.ssl-images-amazon.com/images/I/51RJhTexgxL._SX466_.jpg":[466,91],"https://images-na.ssl-images-amazon.com/images/I/51RJhTexgxL._SX522_.jpg":[522,102]},"variant":"INGR"}]},
                'colorToAsin': {'initial': {}},
                'heroImage': {'initial': []},
                'heroVideo': {'initial': null},
                'holderRatio': 1.0,
