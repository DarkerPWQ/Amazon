<script type="text/javascript">

window.AmazonPopoverImages = {
  snake: 'https://images-na.ssl-images-amazon.com/images/G/01/javascripts/lib/popover/images/snake._CB192571611_.gif',
  btnClose: 'https://images-na.ssl-images-amazon.com/images/G/01/javascripts/lib/popover/images/btn_close._CB192188154_.gif',
  closeTan: 'https://images-na.ssl-images-amazon.com/images/G/01/nav2/images/close-tan-sm._CB192185930_.gif',
  closeTanDown: 'https://images-na.ssl-images-amazon.com/images/G/01/nav2/images/close-tan-sm-dn._CB192185961_.gif',
  loadingBar: 'https://images-na.ssl-images-amazon.com/images/G/01/javascripts/lib/popover/images/loading-bar-small._CB192188123_.gif',
  pixel: 'https://images-na.ssl-images-amazon.com/images/G/01/icons/blank-pixel._CB192192429_.gif'
};
var container = document.createElement("DIV");
container.id = "ap_container";
if (document.body.childNodes.length) {
    document.body.insertBefore(container, document.body.childNodes[0]);
} else {
    document.body.appendChild(container);
}

</script>

