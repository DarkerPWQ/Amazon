<!--     Creating a custom overlay for image not available experience -->
<div class="variationUnavailable unavailableExp">
    <div class="inner">
        
        <div class="a-box a-alert a-alert-error"><div class="a-box-inner a-alert-container"><h4 class="a-alert-heading">Image Unavailable</h4><i class="a-icon a-icon-alert"></i><div class="a-alert-content">
            <span class="a-text-bold">
                Image not available for<br/>Color:
                <span class="unvailableVariation"></span>
            </span>
        </div></div></div>
    </div>
