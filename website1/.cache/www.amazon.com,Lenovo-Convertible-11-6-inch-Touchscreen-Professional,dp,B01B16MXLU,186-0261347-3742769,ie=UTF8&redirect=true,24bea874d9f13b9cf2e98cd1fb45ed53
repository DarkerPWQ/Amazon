    <script type="text/javascript">
  P.when('jQuery', 'a-modal', 'a-checkbox', 'ready').execute(function($, modal, getCheckboxCollection) {
    
    function logRefTag(refTag) {
      $.get("/gp/product/ajax-handlers/reftag.html/ref=" + refTag);
    }
    
    function hidePopover(index) {
      var popover = modal.get("mbbPopover" + index);
      if (popover != null) {
        popover.hide();
      }
    }

    function logCheckBoxRefTag(isChecked, index, warrantyAsin) {
      var suffix = (index+1) + "_B01B16MXLU_" + warrantyAsin;
      if (isChecked) {
        logRefTag("psd_ub_y" + suffix);
      } else {
        logRefTag("psd_ub_n" + suffix);
      }
    }

    jQuery("#buybox .js-abbUsed input[id^=mbb-offeringID-]").click(function() {
      var flag = this.checked;
      getCheckboxCollection('input[id^=mbb-offeringID-]').uncheck();
      this.checked = flag;
      var index = jQuery('#buybox .js-abbUsed input[id^=mbb-offeringID-]').index(this);
      var warrantyAsin = jQuery(this).parents('.abbListItem').find('input[name^=asin]').val();
      logCheckBoxRefTag(flag, index, warrantyAsin);
    });

    jQuery("#buybox #quantity").change(function() {
        var quantity = jQuery(this).val();
        jQuery("#buybox .js-abbNew input[name^=quantity]").each(function() {
          this.value = quantity;
        });
      });

    logRefTag("psd_ub_i_B01B16MXLU");
    
      
      logRefTag("psd_ub_i1_B01B16MXLU_B015D7WRCS");

      $(document).delegate('#mbbAddCoverage3', 'click', function() {
        getCheckboxCollection('input[id^=mbb-offeringID-]').uncheck();
        getCheckboxCollection('input[id^=mbb-offeringID-3]').check();
        logRefTag('psd_ub_y_B01B16MXLU_B015D7WRCS');
        hidePopover("3");
      });

      $(document).delegate('#mbbNoCoverage3', 'click', function() {
        getCheckboxCollection('input[id^=mbb-offeringID-3]').uncheck();
        logRefTag('psd_ub_n_B01B16MXLU_B015D7WRCS');
        hidePopover("3");
      });        
      
      $(document).delegate('#mbbPopoverLink3', 'click', function() {
        logRefTag('psd_ub_pu1_B01B16MXLU_B015D7WRCS');
      });

    
      
      logRefTag("psd_ub_i2_B01B16MXLU_B015D7VLFW");

      $(document).delegate('#mbbAddCoverage4', 'click', function() {
        getCheckboxCollection('input[id^=mbb-offeringID-]').uncheck();
        getCheckboxCollection('input[id^=mbb-offeringID-4]').check();
        logRefTag('psd_ub_y_B01B16MXLU_B015D7VLFW');
        hidePopover("4");
      });

      $(document).delegate('#mbbNoCoverage4', 'click', function() {
        getCheckboxCollection('input[id^=mbb-offeringID-4]').uncheck();
        logRefTag('psd_ub_n_B01B16MXLU_B015D7VLFW');
        hidePopover("4");
      });        
      
      $(document).delegate('#mbbPopoverLink4', 'click', function() {
        logRefTag('psd_ub_pu2_B01B16MXLU_B015D7VLFW');
      });

    
  });
</script>

  
  
  
  


           
       

       
       
       














    
    
    







