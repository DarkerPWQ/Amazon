oot%20%7B%20%0A%09font-style%3A%20italic%3B%20%0A%7D%0A%0A%23productDescription%20.aplus%20table.data%20caption%20%7B%0A%09background%3A%20%23eee%3B%20%0A%09font-size%3A%20.8125em%3B%0A%7D%0A%0A%23productDescription%20.aplus%20table.data%20tr%20td%3Afirst-child%2C%20%23productDescription%20.aplus%20table.data%20tr%20th%3Afirst-child%20%7B%0A%09border-left-width%3A0px%3B%0A%7D%0A%0A%23productDescription%20.aplus%20ul%20%7B%0A%09margin%3A0%200%201em%200%3B%0A%7D%0A%0A%0A%23productDescription%20.aplus%20.center%20%7B%0A%09text-align%3A%20center%3B%0A%7D%0A%0A%23productDescription%20.aplus%20.right%20%7B%0A%09text-align%3A%20right%3B%0A%7D%0A%0A%23productDescription%20.aplus%20%20.sixth-col%2C%0A%23productDescription%20.aplus%20.fourth-col%2C%0A%23productDescription%20.aplus%20.third-col%2C%0A%23productDescription%20.aplus%20.half-col%2C%0A%23productDescription%20.aplus%20.two-third-col%2C%0A%23productDescription%20.aplus%20.three-fourth-col%2C%0A%23productDescription%20.aplus%20.one-col%20%7B%0A%20%20%20%20float%3Aleft%3B%0A%20%20%20%20margin-right%3A%201.6760%25%3B%0A%20%20%20%20overflow%3A%20hidden%3B%0A%7D%0A%0A%23productDescription%20.aplus%20.last%20%7B%0A%20%20%20%20margin-right%3A0px%3B%0A%7D%0A%0A%23productDescription%20.aplus%20.sixth-col%20%7B%0A%20%20%20%20width%3A%2015.080%25%3B%0A%7D%0A%23productDescription%20.aplus%20.fourth-col%20%7B%0A%20%20%20%20width%3A%2023.4637%25%3B%0A%7D%0A%0A%23productDescription%20.aplus%20.third-col%20%7B%0A%20%20%20%20width%3A%2031.8436%25%3B%0A%7D%0A%0A%23productDescription%20.aplus%20.half-col%20%7B%0A%20%20%20%20width%3A%2048.6034%25%3B%0A%7D%0A%0A%23productDescription%20.aplus%20.two-third-col%20%7B%0A%20%20%20%20width%3A%2065.3631%25%3B%0A%7D%0A%0A%23productDescription%20.aplus%20.three-fourth-col%20%7B%0A%20%20%20%20width%3A%2073.7430%25%3B%0A%7D%0A%0A%23productDescription%20.aplus%20.one-col%20%7B%0A%20%20%20%20width%3A%2098.8827%25%3B%0A%20%20%20%20margin-right%3A0%3B%0A%7D%0A%0A%23productDescription%20.aplus%20.last%20%7B%0A%20%20%20%20margin-right%3A0%3B%0A%7D%0A%0A%23productDescription%20.aplus%20%7B%0A%20%20%20%20width%3A%20100%25%3B%0A%20%20%20%20min-width%3A%20895px%3B%0A%7D%0A%0A*%20html%20%23productDescription%20.aplus%20%7B%0A%20%20%20%20width%3A%20expression((document.body.clientWidth%20%3C%20936)%20%3F%20%22895px%22%20%3A%20%22100%25%22%20)%3B%0A%7D%0A%0A%20%20%20%20%3C%2Fstyle%3E%0A%20%20%20%20%3Cbase%20target%3D'_top'%20%2F%3E%0A%20%20%3C%2Fhead%3E%0A%20%20%3Cbody%3E%0A%20%20%3Cdiv%20id%3D%22pdIframeContent%22%3E%0A%0A%0A%0A%0A%20%0A%0A%0A%0A%0A%3Cdiv%20id%3D%22biss-product-description-and-details%22%3E%3C%2Fdiv%3E%0A%0A%3Cdiv%20class%3D%22bucket%22%20id%3D%22productDescription%22%3E%0A%20%20%3Ch2%3EProduct%20Description%3C%2Fh2%3E%0A%0A%3Cdiv%20class%3D%22content%22%3E%0A%0A%0A%20%20%20%20%0A%20%20%20%20%20%20%20%3Ch3%20class%3D%22productDescriptionSource%22%3E%3C%2Fh3%3E%0A%20%20%20%20%20%20%20%3Cdiv%20class%3D%22productDescriptionWrapper%22%3E%0A%20%20%20%20%20%20%20Features%3A%20%0A%3Cbr%20%2F%3E%201)%20Model%3A%20Lenovo%20MA388%20%0A%3Cbr%20%2F%3E%202)%20Display%3A%203.5%20inch%20HVGA%20Screen%20480x320%20pixels%20%0A%3Cbr%20%2F%3E%203)%20Band%20(Network)%3A%202G%20GSM%20900%2F1800%20MHz%20%0A%3Cbr%20%2F%3E%204)%20CPU%3A%20MTK6250%20Single%20Core%20%0A%3Cbr%20%2F%3E%205)%20ROM%3A%2064MB%20%0A%3Cbr%20%2F%3E%206)%20Storage%3A%20Support%20TF%20Card%20extended%2C%20up%20to%208GB%20(Not%20included)%20%0A%3Cbr%20%2F%3E%207)%20SIM%20Card%3A%20Dual%20SIM%20%0A%3Cbr%20%2F%3E%208)%20Camera%3A%20Rear%200.3MP%20Single%20Camera%20%0A%3Cbr%20%2F%3E%209)%20Connectivity%3A%20%0A%3Cbr%20%2F%3E%20-%202%20x%20SIM%20Slot%20(Standard)%20%0A%3Cbr%20%2F%3E%20-%201%20x%20TF%20Card%20Slot%20%0A%3Cbr%20%2F%3E%20-%201%20x%20Mini%20USB%20Port%20%0A%3Cbr%20%2F%3E%2010)%20Bluetooth%3A%20Yes%20%0A%3Cbr%20%2F%3E%2011)%20FM%20Radio%3A%20Yes%2C%20Earphone%20Needed%20%0A%3Cbr%20%2F%3E%2012)%20Message%3A%20SMS%2FMMS%20%0A%3Cbr%20%2F%3E%2013)%20Language%3A%20English%2C%20Chinese%20%0A%3Cbr%20%2F%3E%2014)%20Dimensions%3A%20115mm%20x%2062mm%20x%2018mm%20%0A%3Cbr%20%2F%3E%2015)%20Net%20weight%3A%20150g%20%0A%3Cbr%20%2F%3E%2016)%20Package%20Contents%3A%20%0A%3Cbr%20%2F%3E%20-%201%20x%201900mAh%20Battery%20%0A%3Cbr%20%2F%3E%20-%201%20x%20Charger%20%0A%3Cbr%20%2F%3E%20-%201%20x%20USB%20Cable%20%0A%3Cbr%20%2F%3E%0A%20%20%20%20%20%20%0A%20%20%20%20%20%20%3Cdiv%20class%3D%22emptyClear%22%3E%20%3C%2Fdiv%3E%0A%20%20%20%20%20%20%3C%2Fdiv%3E%0A%20%20%3C%2Fdiv%3E%0A%20%20%20%20%3Cscript%3E%0A%20%20%20%20if(typeof%20P%20!%3D%3D%20'undefined'%20%26%26%20typeof%20P.when%20!%3D%3D%20'undefined')%20%7B%0A%20%20%20%20%20%20%20%20P.when('product-description-fix').execute(function(productDescription)%7B%0A%20%20%20%20%20%20%20%20%20%20%20%20productDescription.fixTableIssue()%3B%20%20%20%20%20%20%0A%20%20%20%20%20%20%20%20%7D)%3B%0A%20%20%20%20%7D%0A%20%20%20%20%3C%2Fscript%3E%0A%3C%2Fdiv%3E%0A%0A%0A%0A%20%20%3C%2Fdiv%3E%0A%20%20%3C%2Fbody%3E%0A%3C%2Fhtml%3E%0A";
    obj.onloadCallback = onloadCallback;
    obj.needWidthAdjust = needWidthAdjust;
    var iframe = obj.createIframe (obj.iframeId, obj.iframeload);
    var wrapper = d.getElementById(obj.appendTo);
    wrapper.innerHTML = '';

    var anchor = d.createElement('div');
    anchor.id = 'biss-product-description-and-details';
    wrapper.appendChild(anchor);

    iframe = wrapper.appendChild(iframe);
    var doc;
    try {
      doc = iframe.contentWindow.document;
    } catch (e) {
      try {
        doc = iframe.contentDocument;
      } catch (ex) {}
    }
    if (doc && doc.open) {
      obj.doc = doc;
      var docOpen = doc.open();
      if (docOpen) {
	docOpen.write(decodeURIComponent(iframeContent));
        doc.close();
      }
    }
  }
  obj.resizeIframe = function() {
    if(hasCustomWidth) {
      setIframeWidth(obj.iframeId);
    }
    nTries = 1;
    setIframeHeight(obj.iframeId);
  }
}).apply(ProductDescriptionIframeResize);

     if(typeof P !== "undefined"){  
      P.when('cf').execute(function(){
        P.when('jQuery').execute(function(jQuery) {
          setTimeout(function() {
                var iframeLoadFn = function() {
                    var $ = jQuery;
                    $("#pd-available").trigger("pdAvailable");
                    $(window).resize(ProductDescriptionIframeResize.resizeIframe);
                };
                    ProductDescriptionIframeResize.initialize(iframeLoadFn, 1);
           },50);
          });
        });
     }
</script>



