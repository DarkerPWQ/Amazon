    <script type="text/javascript">
if (window.P) {
    P.when('A', 'jQuery', 'ready').execute(function(A, jQuery){
        jQuery(".apl_popover_hover").hover(function(){
            var tagContent = "apl_desktop_other_hover";
            jQuery.ajax({
                url: "/gp/collect-coupon/handler/applicable_promotion_list_hover_count.html?ref_=" + tagContent,
                type: "POST",
                data: {
                    tag: tagContent
                },
                dataType: "json",
            });
        });
        var apl_ref_imp = "apl_desktop_imp";
        jQuery.ajax({
            url: "/gp/collect-coupon/handler/applicable_promotion_list_hover_count.html?ref_=" + apl_ref_imp,
            type: "POST",
            data: {
                tag: "apl_desktop_trigger"
            },
            dataType: "json",
        });

        function adjustPromotionMessageWidth() {
            var currentLineW = jQuery(".apl_popover_hover").width() + jQuery(".apl_label").width() + 5 * 2;
            if (!currentLineW) {
                return;
            }

            var maxLineW = jQuery(".apl_popover_hover").parent().parent().width();
            if (!maxLineW || maxLineW <= currentLineW) {
                return;
            }

            var messageW = jQuery(".apl_popover_hover .apl_message").width();
            if (!messageW) {
                return;
            }

            var maxW = maxLineW- currentLineW + messageW;
            var currentMaxW = parseInt(jQuery(".apl_popover_hover .apl_message").css("max-width"));
            if (!isNaN(currentMaxW) && currentMaxW != maxW && maxW >= 200) {
                jQuery(".apl_popover_hover .apl_message").css("max-width", maxW + "px");
            }
        }

        adjustPromotionMessageWidth();
    });
}
</script>


        
        
    


	                
                
            
        
        
        
        
        
        
    
    
    

    















