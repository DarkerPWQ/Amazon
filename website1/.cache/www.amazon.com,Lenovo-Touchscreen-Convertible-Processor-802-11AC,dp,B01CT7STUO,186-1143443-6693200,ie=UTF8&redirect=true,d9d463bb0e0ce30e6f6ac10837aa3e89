        A.declarative('enhanced-content-open-file', 'click', function(event) {
            var pageAction = event.data;
            window.ue.count("ProductInfo" + pageAction, 1);
            A.ajax('/gp/product/ajax-handlers/enhanced-content-popover-metrics.html', {
                method: 'post',
