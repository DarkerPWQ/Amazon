    <script type="text/javascript">
  P.when('jQuery', 'a-modal', 'a-checkbox', 'ready').execute(function($, modal, getCheckboxCollection) {
    
    function logRefTag(refTag) {
      $.get("/gp/product/ajax-handlers/reftag.html/ref=" + refTag);
    }
    
    function hidePopover(index) {
      var popover = modal.get("mbbPopover" + index);
      if (popover != null) {
        popover.hide();
      }
    }

    function logCheckBoxRefTag(isChecked, index, warrantyAsin) {
      var suffix = (index+1) + "_B01CT7STUO_" + warrantyAsin;
      if (isChecked) {
        logRefTag("psd_bb_y" + suffix);
      } else {
        logRefTag("psd_bb_n" + suffix);
      }
    }

    jQuery("#buybox .js-abbNew input[id^=mbb-offeringID-]").click(function() {
      var flag = this.checked;
      getCheckboxCollection('input[id^=mbb-offeringID-]').uncheck();
      this.checked = flag;
      var index = jQuery('#buybox .js-abbNew input[id^=mbb-offeringID-]').index(this);
      var warrantyAsin = jQuery(this).parents('.abbListItem').find('input[name^=asin]').val();
      logCheckBoxRefTag(flag, index, warrantyAsin);
    });

    jQuery("#buybox #quantity").change(function() {
        var quantity = jQuery(this).val();
        jQuery("#buybox .js-abbNew input[name^=quantity]").each(function() {
          this.value = quantity;
        });
      });

    logRefTag("psd_bb_i_B01CT7STUO");
    
      
      logRefTag("psd_bb_i1_B01CT7STUO_B01FVIHNJ6");

      $(document).delegate('#mbbAddCoverage1', 'click', function() {
        getCheckboxCollection('input[id^=mbb-offeringID-]').uncheck();
        getCheckboxCollection('input[id^=mbb-offeringID-1]').check();
        logRefTag('psd_bb_y_B01CT7STUO_B01FVIHNJ6');
        hidePopover("1");
      });

      $(document).delegate('#mbbNoCoverage1', 'click', function() {
        getCheckboxCollection('input[id^=mbb-offeringID-1]').uncheck();
        logRefTag('psd_bb_n_B01CT7STUO_B01FVIHNJ6');
        hidePopover("1");
      });        
      
      $(document).delegate('#mbbPopoverLink1', 'click', function() {
        logRefTag('psd_bb_pu1_B01CT7STUO_B01FVIHNJ6');
      });

    
      
      logRefTag("psd_bb_i2_B01CT7STUO_B00E3EALS0");

      $(document).delegate('#mbbAddCoverage2', 'click', function() {
        getCheckboxCollection('input[id^=mbb-offeringID-]').uncheck();
        getCheckboxCollection('input[id^=mbb-offeringID-2]').check();
        logRefTag('psd_bb_y_B01CT7STUO_B00E3EALS0');
        hidePopover("2");
