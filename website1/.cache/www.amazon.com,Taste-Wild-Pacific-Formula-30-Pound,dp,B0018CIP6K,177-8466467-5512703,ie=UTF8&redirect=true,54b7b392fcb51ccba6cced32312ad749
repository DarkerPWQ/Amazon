    + ",GET@v=" + csaGet + ":u=ms";
    
  var fUrl = [fVip, fApiVersion, fChannelId, fChannelVersion, fAction, fProgramGroup, "action", fMetrics].join("/");

  var visUrlsPlaceHolder = [];
