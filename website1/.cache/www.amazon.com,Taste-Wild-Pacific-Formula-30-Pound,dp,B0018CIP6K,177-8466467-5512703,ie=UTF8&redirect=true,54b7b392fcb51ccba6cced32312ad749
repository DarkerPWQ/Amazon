var handleLoadedSL = function(visUrls) {
  var adsLoaded = visUrls.length;
  slEndTime = new Date().getTime();
  var getTime = slEndTime - slStartTime;

  var requestId = "HD8ZKB5TZ1EQHBAPJERJ";
  var requestTag = "TjaKFEEarjf5JtJF";
  var sessionId = "177-8466467-5512703";
  var marketplace = "US";
  var marketplaceId = "ATVPDKIKX0DER";
  var method = "Detail";
  
