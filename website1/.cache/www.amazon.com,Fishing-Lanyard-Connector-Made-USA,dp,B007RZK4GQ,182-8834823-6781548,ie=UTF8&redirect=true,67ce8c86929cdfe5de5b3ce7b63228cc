<script type="text/JavaScript">
(function () {
    "use strict";

    var P = (window.AmazonUIPageJS ? AmazonUIPageJS : window.P);

    var enableYBHLink = function() {
        P.when('p13n-sc-jQuery').execute(function($) {
            $("#ybh-link").click(function() {
                $.ajax({
                      url: '/gp/recent-history-footer/external/ybh-handler.html/182-8834823-6781548',
                      type: "POST",
                      timeout: 2000,
                      data: {
                           parentSession    : '182-8834823-6781548',
                           relatedRequestId : '2PXD1KVYEWJ5KTJ87BVX',
                           currentPageType  : 'Detail'
                      },
                      dataType: "text",
                      success: function (data, status) {
                           $("#ybh-link").hide();
                           $("#ybh-text-off").hide();
                           $("#ybh-text-on").show();
                      }
                });
            });
        });
    };

    var lazyLoadRHF = function($) {

        // add triggering callBack function if page is part of the experiment (RHF on YA)
        if ('' != ""){
           $.ajax({
               url: '/gp/recent-history-footer/external/rhf-ya-trigger.html/182-8834823-6781548',
               type: "POST",
               data: {
                   relatedRequestId : '2PXD1KVYEWJ5KTJ87BVX'
               },
               dataType: "json"
          });
        }

        // do not load rhf if in control
        if ('1' == 0){
            return 1;
        }

        var loadCarousel = function(html) {
            P.when('p13n-sc-carousel-init', 'rhf-container')
             .execute(function(carouselInitializer, rhfContainer) {
                rhfContainer.html(html);
                carouselInitializer.init(rhfContainer);
                enableYBHLink();
            });
        };

        var handleSuccess = function(data) {
            if (typeof(data) === 'object' && data.success && data.html) {
                loadCarousel(data.html);
            } else {
                handleError();
            }
        };

        var handleError = function() {
            P.when('rhf-container', 'rhf-error')
             .execute(function(rhfContainer, rhfError) {
                rhfContainer.hide();
                rhfError.show();
            });
        };

        $.ajax({
            url: '/gp/recent-history-footer/external/rhf-handler.html/182-8834823-6781548',
            type: "POST",
            timeout: 5000,
            data: {
                             isAUI : (typeof window.P === 'object' && window.P.AUI_BUILD_DATE) ? 1 : 0,
                     parentSession : '182-8834823-6781548',
                  relatedRequestId : '2PXD1KVYEWJ5KTJ87BVX',
                   currentPageType : 'Detail',
                currentSubPageType : 'Glance',
                       excludeASIN : 'QjAwN1JaSzRHUQ==',
                          auiDebug : '',
                          rviAsins : '',
                          rhfState : '',
                          rhfAsins : '',
                       noP13NCache : '',
                         recsAsins : '',
                        customerID : '',
                   testRecsFailure : '',
                    weblabTriggers : '',
           contextMetadataOverride : ''
            },
            dataType: "json",
            success: function (data, status) {
                handleSuccess(data);
            },
            error: function (xhr, status) {
                handleError();
            }
        });

    };

    P.when('p13n-sc-jQuery').execute(function($) {
        P.declare('rhf-container', $("#rhf-container"));
        P.declare('rhf-error', $("#rhf-error"));
        P.declare('rhf-frame', $(".rhf-frame"));
        P.declare('rhf-head', $("#rhf"));
    });

    P.when('p13n-sc-jQuery', 'rhf-container', 'rhf-frame',
            'p13n-sc-call-on-visible', 'p13n-sc-carousel-init','rhf-head')
     .execute(function($, rhfContainer, rhfFrame, callOnVisible, carouselInitializer, rhf) {
	if (1 == 1){
	    rhfFrame.show();
        }
        callOnVisible.register(rhf, function () { lazyLoadRHF($); }, { distance: '-400' });
    });

})();
</script>

